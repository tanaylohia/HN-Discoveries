{
  "functions": {
    "api/discoveries.py": {
      "runtime": "python@3.9"
    },
    "api/refresh.py": {
      "runtime": "python@3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}